[MASTER]
ignore=CVS
persistent=yes
load-plugins=
jobs=1
unsafe-load-any-extension=no
extension-pkg-whitelist=
optimize-ast=no

[MESSAGES CONTROL]
confidence=
disable=parameter-unpacking,raw_input-builtin,useless-suppression,unpacking-in-except,dict-iter-method,getslice-method,long-builtin,delslice-method,map-builtin-not-iterating,coerce-method,coerce-builtin,execfile-builtin,round-builtin,old-octal-literal,using-cmp-argument,dict-view-method,cmp-method,next-method-called,indexing-exception,unicode-builtin,oct-method,old-raise-syntax,old-division,xrange-builtin,print-statement,cmp-builtin,old-ne-operator,nonzero-method,metaclass-assignment,import-star-module-level,backtick,unichr-builtin,intern-builtin,reload-builtin,apply-builtin,file-builtin,basestring-builtin,raising-string,long-suffix,zip-builtin-not-iterating,reduce-builtin,hex-method,no-absolute-import,standarderror-builtin,range-builtin-not-iterating,setslice-method,suppressed-message,input-builtin,buffer-builtin,filter-builtin-not-iterating,bad-continuation,protected-access,arguments-differ,bad-whitespace

[REPORTS]
output-format=text
files-output=no
reports=yes
evaluation=10.0 - ((float(5 * error + warning + refactor + convention) / statement) * 10)

[LOGGING]
logging-modules=logging

[FORMAT]
max-line-length=120
ignore-long-lines=^\s*(# )?<?https?://\S+>?$
single-line-if-stmt=no
no-space-check=trailing-comma,dict-separator
max-module-lines=1250
indent-string=""
indent-after-paren=0
expected-line-ending-format=

[BASIC]
bad-functions=map,filter
good-names=i,j,k,b,f,v,m,n,p,d,hh,st,ex,Run,_
bad-names=foo,bar,baz,toto,tutu,tata
name-group=
include-naming-hint=no
class-rgx=[A-Z_][a-zA-Z0-9]+$
class-name-hint=[A-Z_][a-zA-Z0-9]+$
inlinevar-rgx=[A-Za-z_][A-Za-z0-9_]*$
inlinevar-name-hint=[A-Za-z_][A-Za-z0-9_]*$
module-rgx=(([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$
module-name-hint=(([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$
argument-rgx=[a-z_][a-z0-9_]{2,30}$
argument-name-hint=[a-z_][a-z0-9_]{2,30}$
method-rgx=[a-z_][a-z0-9_]{2,30}$
method-name-hint=[a-z_][a-z0-9_]{2,30}$
variable-rgx=[a-z_][a-z0-9_]{2,30}$
variable-name-hint=[a-z_][a-z0-9_]{2,30}$
const-rgx=(([A-Z_][A-Z0-9_]*)|(__.*__))$
const-name-hint=(([A-Z_][A-Z0-9_]*)|(__.*__))$
attr-rgx=[a-z_][a-z0-9_]{2,30}$
attr-name-hint=[a-z_][a-z0-9_]{2,30}$
function-rgx=[a-z_][a-z0-9_]{2,30}$
function-name-hint=[a-z_][a-z0-9_]{2,30}$
class-attribute-rgx=([A-Za-z_][A-Za-z0-9_]{2,30}|(__.*__))$
class-attribute-name-hint=([A-Za-z_][A-Za-z0-9_]{2,30}|(__.*__))$
no-docstring-rgx=^_
docstring-min-length=-1

[ELIF]
max-nested-blocks=5

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no

[TYPECHECK]
ignore-mixin-members=yes
ignored-modules=
ignored-classes=
generated-members=

[VARIABLES]
init-import=no
dummy-variables-rgx=_$|dummy
additional-builtins=
callbacks=cb_,_cb

[MISCELLANEOUS]
notes=FIXME,XXX,TODO

[SPELLING]
spelling-dict=
spelling-ignore-words=
spelling-private-dict-file=
spelling-store-unknown-words=no

[IMPORTS]
deprecated-modules=optparse
import-graph=
ext-import-graph=
int-import-graph=

[DESIGN]
max-args=12
ignored-argument-names=_.*
max-locals=15
max-returns=6
max-branches=12
max-statements=50
max-parents=12
max-attributes=20
min-public-methods=2
max-public-methods=25
max-bool-expr=5

[CLASSES]
defining-attr-methods=__init__,__new__,setUp
valid-classmethod-first-arg=cls
valid-metaclass-classmethod-first-arg=mcs
exclude-protected=_asdict,_fields,_replace,_source,_make

[EXCEPTIONS]
overgeneral-exceptions=Exception
